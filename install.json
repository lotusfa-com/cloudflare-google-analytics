{
  "$schema": "http://json.schemastore.org/install",
  "resources": {
    "body": [
      {
        "type": "script",
        "src": "./build/index.js"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["_default"],
        "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "tracker_id": {
        "title": "Google Tracker ID",
        "description": "Add your tracker id here",
        "type": "string",
        "order": 0,
        "default": "UA-XXXXX-Y"
      }
    }
  }
}
